 #include<bits/stdc++.h>



 static boolean isUnique2(String str){

        int checker = 0;
        for(int i=0;i<str.length();i++){

            int val = str.charAt(i) - 'a'; // c -> 2

            if( (checker & (1<<val)) > 0 )
                    return false;

            checker |= (1<<val);

        }
        return true;

    }